{
    "id": "1",
    "type": "widget",
    "name": "Simple Demo",
    "auther": "PixelBoard Team",
    "version": "1.0.0",
    "description": "A simple demo application for PixelBoard.",
    "size": [128,128],
    "fields": [
        {
            "id": "text", 
            "name": "text",
            "type": "text",
            "desc": "Text to display",
            "default": "Hello, PixelBoard!"
        },
        {
            "id": "color", 
            "name": "color",
            "type": "color",
            "desc": "Text color",
            "default": "#FFFFFF"
        },
        {
            "id": "dropdown", 
            "name": "dropdown",
            "type": "dropdown",
            "desc": "Choose an option",
            "options": [
                {
                    "display": "Option 1",
                    "value": "0"
                },
                {
                    "display": "Option 2",
                    "value": "1"
                },
                {
                    "display": "Option 3",
                    "value": "2"
                }
            ],
            "default": "0"
        },
        {
            "id": "toggle",
            "name": "toggle",
            "type": "toggle",
            "desc": "Enable or disable feature",
            "default": true
        },
        {
            "id": "number",
            "name": "number",
            "type": "number",
            "desc": "Enter a number",
            "default": 42.5,
            "min": 0,
            "max": 100
        },
        {
            "id": "slider",
            "name": "slider",
            "type": "slider",
            "desc": "Adjust the value",
            "default": 50,
            "min": 0,
            "max": 100
        },
        {
            "id": "checkbox",
            "name": "checkbox",
            "type": "checkbox",
            "desc": "Select options(multiple allowed)",
            "options": [
                {
                    "display": "Option A",
                    "value": "A"
                },
                {
                    "display": "Option B",
                    "value": "B"
                },
                {
                    "display": "Option C",
                    "value": "C"
                }
            ],
            "default"  : ["A", "B"]
        },
        {
            "id": "files",
            "name": "files",
            "type": "files",
            "desc": "Upload files",
            "accept": ["bmp","jpg","png"],
            "counts": [0,1],
            "default": []
        }
    ],
    "preview": [""]
}